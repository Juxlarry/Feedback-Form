<!--Modal Page Rendering As JS -->
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel"><%=  image_tag ("logo.png"), :style => "width: 400px" %>
                <p id="p-tag"> Thanks You</p>
        </h4>
</div>

<!--Rating Stars Section -->
    <div class="modal-body">
        <div class="header-modal"> Overall Rating </div>
           <div class="rating-stars"> 
            <% @final_rating.to_i.times do |h|  %>

                 <%=  image_tag ("starry.png"), :style => "width: 60px" %>
               
            <% end %>
            </div>  
    </div>
<!--Ratings Stars Section End -->

    <div class="modal-footer">
    
    <p> <span class="footer-msg"> ... We Promise To Keep Serving You Better </span>
     <button type="button" class="btn btn-default" data-dismiss="modal">Close</button> </p>
    
    </div>
            
       
    
           